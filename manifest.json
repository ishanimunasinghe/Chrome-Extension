{
    "manifest_version": 3,
    "name": "Clicktionary",
    "description": "Search for the definition and synonyms of a word.",
    "version": "1.0",

    "action":{
        "default_popup": "popup/popup.html"
    },
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.jpg"
    },

    "permissions": [
        "storage"
    ]
    
}