{
    "manifest_version": 3,
    "name": "CoolBackground",
    "description": "Add a custom background to Google.com",
    "version": "1.0",
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.jpg"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.www.google.com/*"
            ],
            "css": [
                "main.css"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}